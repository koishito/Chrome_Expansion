{
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2,
    "name": "extensiontest",
    "version": "0.2",
    "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
      "default_popup":"popup.html"
  },
  "permissions": [
      "tabs"
    ]
  }