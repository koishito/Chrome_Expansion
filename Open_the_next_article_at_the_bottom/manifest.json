{
  "manifest_version": 2,
  "name": "Open_the_next_article_at_the_bottom",
  "version": "0.1",
  "description": "Open the next article at the bottom.",
  "browser_action": {
    "default_title": "Open_the_next_article_at_the_bottom"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs", "declarativeContent", "http://*/*", "https://*/*"]
}
